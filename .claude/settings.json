{
  "enabledPlugins": {
    "hookify@claude-plugins-official": true
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Edit|Write|Bash",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/enforce-orchestrator-restrictions.py"
          }
        ]
      }
    ]
  }
}
